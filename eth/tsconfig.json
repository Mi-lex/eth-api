{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["ES2020.BigInt"],
    "target": "es2020",
    "sourceMap": true,
    "outDir": "./dist",
    "incremental": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": true
  }
}
